{
  "name": "dxaws-cdk",
  "version": "1.0.17",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "clean": "rm -rf dist",
    "prepare-old": "npm test && npm run build",
    "test": "jest --config ./test/jest.config.js",
    "publish:local": "yalc publish --push",
    "watch": "tsc -w & nodemon -w dist -x 'yalc push'",
    "unpublish:local": "yalc remove --all"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@aws-cdk/assertions": "^1.204.0",
    "@types/jest": "^30.0.0",
    "@types/node": "24.4.0",
    "aws-cdk-lib": "^2.215.0",
    "constructs": "^10.4.2",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "ts-patch": "^3.3.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "typescript-transform-paths": "^3.5.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.167.0",
    "constructs": "^10.0.0"
  }
}
