{
  "name": "dxaws-cdk",
  "version": "1.0.17",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "test": "jest --config ./test/jest.config.js",
    "publish:local": "npm run build && yalc publish --push",
    "watch": "tsc -w & nodemon -w dist -x 'yalc push'",
    "unpublish:local": "yalc remove --all"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@aws-cdk/assertions": "^1.203.0",
    "@types/jest": "^29.5.12",
    "@types/node": "22.5.4",
    "aws-cdk-lib": "^2.171.1",
    "constructs": "^10.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-patch": "^3.2.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^3.14.0",
    "typedoc": "^0.27.2",
    "typedoc-plugin-markdown": "^4.2.10",
    "typescript": "^5.7.2",
    "typescript-transform-paths": "^3.5.2"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.167.0",
    "constructs": "^10.0.0"
  }
}
